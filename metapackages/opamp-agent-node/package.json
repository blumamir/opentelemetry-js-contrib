{
  "name": "@opentelemetry/opamp-agent-node",
  "version": "0.0.1",
  "description": "opamp nodejs agent for remote managing of opentelemetry SDK and instrumentations",
  "author": "OpenTelemetry Authors",
  "homepage": "https://github.com/open-telemetry/opentelemetry-js-contrib/tree/main/metapackages/opamp-agent-node#readme",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "exports": {
    ".": "./build/src/index.js",
    "./register": "./build/src/register.js"
  },
  "repository": "open-telemetry/opentelemetry-js-contrib",
  "scripts": {
    "clean": "rimraf build/*",
    "compile": "npm run protos && tsc -p .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "precompile": "tsc --version && lerna run version:update --scope @opentelemetry/opamp-agent-node --include-dependencies",
    "prewatch": "npm run precompile",
    "prepare": "npm run compile",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.ts'",
    "watch": "tsc -w",
    "submodule": "git submodule sync --recursive && git submodule update --init --recursive",
    "protos": "npm run submodule && npm run protos:generate",
    "protos:generate": "node scripts/generate-protos.js"
  },
  "bugs": {
    "url": "https://github.com/open-telemetry/opentelemetry-js-contrib/issues"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.4.1"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.4.1",
    "@types/mocha": "7.0.2",
    "@types/node": "18.6.5",
    "@types/sinon": "10.0.18",
    "mocha": "7.2.0",
    "nyc": "15.1.0",
    "rimraf": "5.0.5",
    "sinon": "15.2.0",
    "ts-mocha": "10.0.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "axios": "^1.6.1",
    "protobufjs": "^7.2.3"
  }
}
